{
  "name": "Rapid Content Management Framework",
  "description": "Creating and modifying content with as minimum coding as possible.",
  "keywords": [],
  "homepage": "https://github.com/wimulkeman/Rapid-Content-Management-Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Wim Ulkeman",
      "role": "Developer"
    }
  ],
  "require": {
    "php" : ">=5.4.0",
    "symfony/class-loader": "~2.6",
    "symfony/http-foundation": "~2.6",
    "doctrine/orm": "~2.5",
    "symfony/yaml": "~2.6",
    "twig/twig": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.*"
  },
  "autoload": {
    "psr-4": {
      "Bearwulf\\RCMF\\": "src/Bearwulf/RCMF"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bearwulf\\RCMF\\Test\\": "tests/Bearwulf/RCMF"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
